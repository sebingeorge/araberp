@model IList<ArabErp.Domain.SupplyOrderRegister>

<div class="main-grid main-grid-top scroll1">
    <table class="table table-bordered">
        <thead>
            <tr class="grid_heading">
                <th width="8%"><h1>SO No</h1></th>
                <th width="8%"><h1>Date</h1></th>
                <th width="20%"><h1>Supplier</h1></th>
                <th width="25%"><h1>Material/Spares</h1></th>
                <th class="text-left" width="5%"><h1>Unit</h1></th>
                <th class="text-right" width="8%"><h1>Qty As Per SO</h1></th>
                 <th class="text-right" width="8%"><h1>Qty Recevied</h1></th>
                <th class="text-right" width="8%"><h1>Difference</h1></th>
                <th class="text-left" width="8%"><h1>Status</h1></th>
            </tr>
        </thead>

        <tbody class="grid_row">
      
                @foreach (var item in Model)
                {
                    <tr>
                        <td class="grid-cell">@item.SupplyOrderNo</td>
                        <td class="grid-cell">@item.SupplyOrderDate.ToString("dd/MMM/yyyy")</td>
                        <td class="grid-cell">@item.SupplierName</td>
                        <td class="grid-cell">@item.ItemName</td>
                        <td class="grid-cell text-left">@item.UnitName</td>
                        <td class="grid-cell text-right">@item.OrderedQty</td>
                        <td class="grid-cell text-right">@item.ReceviedQty</td>
                        <td class="grid-cell text-right">@item.BalanceQty</td>
                        <td class="grid-cell text-left">@item.STATUS</td>

                    </tr>

                }

        </tbody>

    </table>
</div>
<div class="row-count1">
    <h5>Row Count:- @Model.Count()</h5>
</div>

