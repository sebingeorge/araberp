
@{
    ViewBag.Title = "create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="h4bgcolor ">
    <h4>Empolyee Category</h4>
</div>

<div class="container">
    <div class="row ">
        <div class="col-md-4">
        </div>

        <div class=" col-md-4">
            <form role="form">

                <div class=" row form-group">
                    <div class=" col-md-12">
                        <label for="EmpCategoryRefNo">Code</label>
                        <input type="text" id="EmpCategoryRefNo" autocomplete=" off" class="form_input " />
                    </div>

                </div>

                <div class="row form-group ">
                    <div class="col-md-12">
                        <label for="EmpCategoryName">Category</label>
                        <input type="text" id="EmpCategoryName" autocomplete="off" class="form_input " />
                    </div>
                </div>

                <div class="col-md-12">
                    <div style=" float :right" class="alignCenter">
                        <button type="submit" class="button-style">Exit</button>
                    </div>

                    <div style=" float :right" class="alignCenter">
                        <button type="submit" class="button-style">Save</button>
                    </div>
                </div>

            </form>
        </div>
    </div>
</div>