@model ArabErp.Domain.Item
@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script type="text/javascript">
    $(function () {
        $("input[type='text'], select, textarea").attr('disabled', 'disabled');
    });
    $(document).ready(function () {
            @*var catId = '@Model.ItemCategoryId';
            var groupId = '@Model.ItemGroupId';
            var subgroupId = '@Model.ItemSubGroupId';*@
        $("#ItemCategoryId").val('@Model.ItemCategoryId');
        $("#ItemGroupId").val('@Model.ItemGroupId');
        $("#ItemSubGroupId").val('@Model.ItemSubGroupId');

        toastr.options = {
            "closeButton": true,
            "positionClass": "toast-bottom-full-width",
            "preventDuplicates": false,
            "onclick": null,

            "timeOut": "0",
            "extendedTimeOut": "1000",

            "showEasing": "swing",
            "hideEasing": "linear",
            "showMethod": "fadeIn",
            "hideMethod": "fadeOut"
        }
        if ('@TempData["success"]' != "" && '@TempData["ItemRefNo"]' != "") {
            toastr.success('@TempData["success"]' + '  Item Code is : ' + '@TempData["ItemRefNo"]');
        }
        else if ('@TempData["error"]' != "") {
            toastr.error('@TempData["error"]');
        }

    });
</script>


  
   
    @Html.Partial("_Form")


