
@model IList<ArabErp.Domain.SalesRegister>
<table class="table table-bordered">
    <thead>
        <tr class="grid_heading">
            <th width="10%"><h1>SO.NO.</h1></th>
            <th width="35%"><h1>Work Description</h1></th>
            <th width="5%" class="text-right"><h1>Qty.</h1></th>
            <th width="5%" class="text-right"><h1>Amount</h1></th>
            <th width="5%" class="text-right"><h1>%</h1></th>

        </tr>
    </thead>
    @foreach (var item in Model)
    {
        <tbody class="grid_row">
            <tr>
                <td class="grid-cell">@item.SaleOrderRefNo</td>
                <td class="grid-cell">@item.WorkDescrShortName</td>
                <td class="grid-cell text-right">@item.Quantity</td>
                <td class="grid-cell text-right">@item.Amount</td>
                <td class="grid-cell text-right">@item.Perc</td>

            </tr>
        </tbody>
    }
</table>