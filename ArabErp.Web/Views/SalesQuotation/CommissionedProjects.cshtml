@model IList<ArabErp.Domain.ProjectCompletion>

@{
    ViewBag.Title = "Commissioned Projects";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script type="text/javascript">

    function Filter() {
        var url = '@Url.Action("CommissionedProjectsGrid")';
        //$.getJSON()
    }

    function OnItemSelected(id) {
        var url = '@Url.Action("CreateAfterSalesProject")';
        url += "/" + id;
        window.location = url;
    }

    function PreviousList() {
        var url = '@Url.Action("PreviousList")';
        url += "?ProjectBased=1&AfterSales=1";
        window.location = url;
    }

</script>

<div class="row">

    <div class="form-head1">
        <h4>Commissioned Projects</h4>
    </div>

    <div class="main-part pad1">
        <div id="commissioned-projects-table">
            @Html.Partial("_CommissionedProjectsGrid", Model)
        </div>
        <div class="row">
            <div class="col-md-12 hrpart">
                <input class="form-button" type="button" value="Previous" onclick="PreviousList()" />
            </div>
        </div>
    </div>
</div>

